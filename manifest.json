{
    "manifest_version": 2,
    "name": "Canadiana",
    "version": "1.0.2",
  
    "description": "Redirects American websites to the equivalent Canadian website.",
  
    "icons": {
      "48": "icons/canada-48.png"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "canadiana@canada.com",
            "strict_min_version": "120.0"
        }
    }
  
    "content_scripts": [
      {
        "matches": ["https://*.music.apple.com/*"],
        "js": ["src/apple-music.js"]
      }
    ]
  }
